! Xft
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

! URxvt
URxvt*internalBorder: 2
URxvt*scrollBar:      false
URxvt*saveLines:      10000
URxvt*visualBell:     false
URxvt*loginShell:     true

! no window decoration
URxvt*borderLess:     true
! sets $TERM
URxvt*termName: screen-256color

! geometry and font
URxvt*geometry: 80Ã—25
URxvt*font:     xft:Monaco:pixelsize=13

! enabling clickable urls on the terminal
URxvt*perl-ext-common: default,matcher
URxvt*matcher.button:  1
URxvt*urlLauncher:     firefox
URxvt*colorUL:         #86a2be

! dont jump to the bottom when tty gets new lines or output
URxvt*scrollWithBuffer: true
URxvt*scrollTtyOutput:  true

! change default colors
URxvt*background: #000000
URxvt*foreground: #eeeeee

! black
URxvt*color0:  #000000
URxvt*color8:  #252525

! red
URxvt*color1:  #C12121
URxvt*color9:  #E50E0E

! green
URxvt*color2:  #597b20
URxvt*color10: #89b83f

! yellow
URxvt*color3:  #Ded838
URxvt*color11: #efef60

! blue
URxvt*color4:  #265997
URxvt*color12: #3F6FD0

! magenta
URxvt*color5:  #706c9a
URxvt*color13: #826ab1

! cyan
URxvt*color6:  #69a2b0
URxvt*color14: #a1cdcd

! white
URxvt*color7:  #BBBBBB
URxvt*color15: #EEEEEF

! Yeahconsole.
yeahconsole*term:          urxvtc
yeahconsole*screenWidth:   1366
yeahconsole*handleWidth:   4
yeahconsole*consoleHeight: 38
yeahconsole*stepSize:      0
yeahconsole*toggleKey:     None+F1
